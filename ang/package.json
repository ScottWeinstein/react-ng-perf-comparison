{
  "name": "ang",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean:docs-ang && ng build --prod --aot --progress --output-path ../docs/ang/ --base-href .",
    "build:serve": "npm run build && npm run serve:docs-ang",
    "build:verbose": "ng build --prod --aot --progress --verbose --statsJson",
    "clean:docs-ang": "npm run rimraf -- ../docs/ang/*",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "mv": "mv",
    "ng": "ng",
    "rimraf": "rimraf",
    "serve": "serve",
    "serve:docs-ang": "npm run serve -- -p 5002 -s build",
    "start": "ng serve",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.1.0",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/language-service": "^4.1.3",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mv": "^2.1.1",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "serve": "^5.2.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
